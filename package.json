{
  "name": "thanksgiving-website-ts",
  "version": "3.1.19",
  "description": "Thanksgiving Menu Website - TypeScript Version",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc && cp -r src/templates dist/",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:photo": "jest --config jest.config.photo.js",
    "test:photo:unit": "jest tests/unit/photoComponent.test.js --config jest.config.photo.js",
    "test:photo:api": "jest tests/api/photoApi.test.js --config jest.config.photo.js",
    "test:photo:e2e": "playwright test tests/e2e/photo-upload.test.js",
    "test:photo:all": "node scripts/run-photo-tests.js",
    "test:coverage": "jest --coverage",
    "test:smoke": "jest tests/smoke/",
    "test:smoke:api": "jest tests/smoke/api.test.ts",
    "test:smoke:db": "jest tests/smoke/database.test.ts",
    "test:smoke:runner": "ts-node scripts/run-smoke-tests.ts",
    "test:smoke:test": "TEST_BASE_URL=https://thanksgiving-test-test.up.railway.app ts-node scripts/run-smoke-tests.ts",
    "test:smoke:production": "TEST_BASE_URL=https://thanksgiving-prod-production.up.railway.app ts-node scripts/run-smoke-tests.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:smoke": "playwright test tests/e2e/smoke.spec.ts",
    "version:patch": "ts-node scripts/version-manager.ts patch",
    "version:minor": "ts-node scripts/version-manager.ts minor",
    "version:major": "ts-node scripts/version-manager.ts major",
    "version:check": "ts-node scripts/version-manager.ts",
    "db:migrate": "prisma migrate deploy",
    "deploy:railway": "bash scripts/railway-deploy.sh",
    "deploy:railway:production": "bash scripts/railway-deploy-production.sh",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:status": "prisma migrate status",
    "validate:migrations": "ts-node scripts/validate-migrations.ts",
    "verify:schema": "npx prisma db execute --stdin <<< \"SELECT table_name FROM information_schema.tables WHERE table_schema = 'public' ORDER BY table_name;\"",
    "db:push": "prisma db push",
    "db:seed": "ts-node scripts/insert-sample-data.ts",
    "upload:images": "ts-node scripts/upload-images-to-railway.ts",
    "fix:filenames": "ts-node scripts/fix-menu-filenames.ts",
    "deploy": "npm run build && npm run db:migrate && npm start",
    "deploy:dev": "npm run build && npm run db:migrate:dev && npm run dev",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:generate": "prisma generate",
    "db:seed:force": "ts-node scripts/load-all-menus.ts --force",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "db:studio": "prisma studio",
    "db:backup": "./db-protect.sh backup",
    "db:restore": "./db-protect.sh restore",
    "db:list": "./db-protect.sh list",
    "db:check": "./db-protect.sh status",
    "db:safe-reset": "./db-protect.sh backup && prisma migrate reset",
    "prod:backup": "./prod-protect.sh backup",
    "prod:status": "./prod-protect.sh status",
    "prod:warn": "./prod-protect.sh warn",
    "prod:safe-migrate": "./prod-protect.sh backup && prisma migrate deploy",
    "railway:safe-deploy": "./railway-safe.sh deploy",
    "railway:safe-backup": "./railway-safe.sh backup",
    "railway:safe-migrate": "./railway-safe.sh migrate",
    "railway:status": "./railway-safe.sh status",
    "test:backup": "./test-protect.sh backup",
    "test:status": "./test-protect.sh status",
    "test:warn": "./test-protect.sh warn",
    "test:sync": "./test-protect.sh sync",
    "test:safe-migrate": "./test-protect.sh backup && prisma migrate deploy",
    "migrate:s3:menu-images": "ts-node scripts/migrate-menu-images-to-s3.ts",
    "migrate:s3:menu-images:live": "ts-node scripts/migrate-menu-images-to-s3.ts --live",
    "upload:local-menus": "ts-node scripts/upload-local-menus-to-s3.ts",
    "upload:local-menus:live": "ts-node scripts/upload-local-menus-to-s3.ts --live",
    "upload:local-menus:prod": "ts-node scripts/upload-local-menus-to-production-s3.ts",
    "upload:local-menus:prod:live": "ts-node scripts/upload-local-menus-to-production-s3.ts --live",
    "upload:public-images:prod": "ts-node scripts/upload-public-images-to-production-s3.ts",
    "upload:public-images:prod:live": "ts-node scripts/upload-public-images-to-production-s3.ts --live",
    "test:upload-endpoints": "node scripts/test-upload-endpoints.js",
    "update:db-s3-urls": "ts-node scripts/update-database-with-s3-urls.ts",
    "update:db-s3-urls:live": "ts-node scripts/update-database-with-s3-urls.ts --live",
    "fix:s3-urls": "ts-node scripts/fix-s3-urls.ts",
    "fix:s3-urls:live": "ts-node scripts/fix-s3-urls.ts --live",
    "clean": "rimraf dist",
    "postbuild": "npm run db:generate",
    "deploy:production": "ts-node scripts/deploy-production.ts",
    "deploy:production:help": "ts-node scripts/deploy-production.ts --help"
  },
  "keywords": [
    "thanksgiving",
    "menu",
    "family",
    "typescript",
    "prisma",
    "express",
    "railway"
  ],
  "author": "Bob Maguire",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/s3-request-presigner": "^3.901.0",
    "@prisma/client": "^5.7.1",
    "@types/nodemailer": "^7.0.2",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-pg-simple": "^9.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.9",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/express-ejs-layouts": "^2.5.4",
    "@types/express-session": "^1.17.10",
    "@types/jest": "^29.5.8",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.10.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "jsdom": "^27.0.0",
    "prettier": "^3.1.0",
    "prisma": "^5.7.1",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
